# Arquivo de configuração de ambiente
# Copie este arquivo para .env e ajuste os valores conforme necessário

# API Configuration
API_URL=https://catfact.ninja/facts?limit=10

# MinIO Configuration
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=datalakeuser
MINIO_SECRET_KEY=datalakepassword
MINIO_BUCKET=bronze-landing

# Airflow Configuration (se usar localmente)
AIRFLOW__CORE__DAGS_FOLDER=/path/to/mudar-depois/dags
AIRFLOW__CORE__LOAD_EXAMPLES=False

# PySpark Configuration
SPARK_HOME=/usr/local/spark
PYSPARK_PYTHON=python3
PYSPARK_DRIVER_PYTHON=python3

# Logging
LOG_LEVEL=INFO
